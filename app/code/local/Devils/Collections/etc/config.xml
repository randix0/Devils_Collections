<?xml version="1.0"?>
<config>
    <modules>
        <Devils_Collections>
            <version>0.1.0.0</version>
        </Devils_Collections>
    </modules>
    <global>
        <blocks>
            <devils_collections>
                <class>Devils_Collections_Block</class>
            </devils_collections>
        </blocks>
        <resources>
            <devils_collections_setup>
                <setup>
                    <module>Devils_Collections</module>
                    <class>Devils_Collections_Model_Resource_Setup</class>
                </setup>
            </devils_collections_setup>
        </resources>
        <models>
            <devils_collections>
                <class>Devils_Details_Model</class>
                <!--<resourceModel>devils_details_resource</resourceModel>-->
            </devils_collections>
            <!--
            <devils_collections_resource>
                <class>Devils_Details_Model_Resource</class>
                <entities>
                    <details_source>
                        <table>devils_collections_entity</table>
                    </details_source>
                </entities>
            </devils_collections_resource>
            -->
        </models>
        <helpers>
            <devils_collections>
                <class>Devils_Collections_Helper</class>
            </devils_collections>
        </helpers>
    </global>
    <frontend>
        <layout>
            <updates>
                <devils_collections>
                    <file>devils/collections.xml</file>
                </devils_collections>
            </updates>
        </layout>
    </frontend>
</config>